
select /* query_templates/query61.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0000.i0061.1.0:CF! */  promotions,total,promotions/total*100
from
  (select sum(ss_ext_sales_price) promotions
   from  public.store_sales
        ,public.store
        ,public.promotion
        ,public.date_dim
        ,public.customer
        ,public.customer_address 
        ,public.item
   where ss_sold_date_sk = d_date_sk
   and   ss_store_sk = s_store_sk
   and   ss_promo_sk = p_promo_sk
   and   ss_customer_sk= c_customer_sk
   and   ca_address_sk = c_current_addr_sk
   and   ss_item_sk = i_item_sk 
   and   ca_gmt_offset = -6
   and   i_category = 'Home'
   and   (p_channel_dmail = 'Y' or p_channel_email = 'Y' or p_channel_tv = 'Y')
   and   s_gmt_offset = -6
   and   d_year = 2000
   and   d_moy  = 11) promotional_sales,
  (select sum(ss_ext_sales_price) total
   from  public.store_sales
        ,public.store
        ,public.date_dim
        ,public.customer
        ,public.customer_address
        ,public.item
   where ss_sold_date_sk = d_date_sk
   and   ss_store_sk = s_store_sk
   and   ss_customer_sk= c_customer_sk
   and   ca_address_sk = c_current_addr_sk
   and   ss_item_sk = i_item_sk
   and   ca_gmt_offset = -6
   and   i_category = 'Home'
   and   s_gmt_offset = -6
   and   d_year = 2000
   and   d_moy  = 11) all_sales
order by promotions, total
limit 100;

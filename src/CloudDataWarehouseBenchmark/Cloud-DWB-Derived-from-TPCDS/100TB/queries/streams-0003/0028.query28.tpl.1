
select /* query_templates/query28.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0003.i0028.1.0:CF! */  *
from (select avg(ss_list_price) B1_LP
            ,count(ss_list_price) B1_CNT
            ,count(distinct ss_list_price) B1_CNTD
      from public.store_sales
      where ss_quantity between 0 and 5
        and (ss_list_price between 2 and 2+10 
             or ss_coupon_amt between 2956 and 2956+1000
             or ss_wholesale_cost between 7 and 7+20)) B1,
     (select avg(ss_list_price) B2_LP
            ,count(ss_list_price) B2_CNT
            ,count(distinct ss_list_price) B2_CNTD
      from public.store_sales
      where ss_quantity between 6 and 10
        and (ss_list_price between 80 and 80+10
          or ss_coupon_amt between 3391 and 3391+1000
          or ss_wholesale_cost between 44 and 44+20)) B2,
     (select avg(ss_list_price) B3_LP
            ,count(ss_list_price) B3_CNT
            ,count(distinct ss_list_price) B3_CNTD
      from public.store_sales
      where ss_quantity between 11 and 15
        and (ss_list_price between 164 and 164+10
          or ss_coupon_amt between 16700 and 16700+1000
          or ss_wholesale_cost between 68 and 68+20)) B3,
     (select avg(ss_list_price) B4_LP
            ,count(ss_list_price) B4_CNT
            ,count(distinct ss_list_price) B4_CNTD
      from public.store_sales
      where ss_quantity between 16 and 20
        and (ss_list_price between 66 and 66+10
          or ss_coupon_amt between 11071 and 11071+1000
          or ss_wholesale_cost between 76 and 76+20)) B4,
     (select avg(ss_list_price) B5_LP
            ,count(ss_list_price) B5_CNT
            ,count(distinct ss_list_price) B5_CNTD
      from public.store_sales
      where ss_quantity between 21 and 25
        and (ss_list_price between 18 and 18+10
          or ss_coupon_amt between 8128 and 8128+1000
          or ss_wholesale_cost between 66 and 66+20)) B5,
     (select avg(ss_list_price) B6_LP
            ,count(ss_list_price) B6_CNT
            ,count(distinct ss_list_price) B6_CNTD
      from public.store_sales
      where ss_quantity between 26 and 30
        and (ss_list_price between 67 and 67+10
          or ss_coupon_amt between 3862 and 3862+1000
          or ss_wholesale_cost between 74 and 74+20)) B6
limit 100;

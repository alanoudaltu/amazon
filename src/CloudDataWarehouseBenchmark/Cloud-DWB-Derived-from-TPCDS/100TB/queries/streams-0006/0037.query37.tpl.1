
select /* query_templates/query37.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0006.i0037.1.0:CF! */  i_item_id
       ,i_item_desc
       ,i_current_price
 from public.item,public.inventory,public.date_dim, public.catalog_sales
 where i_current_price between 17 and 17 + 30
 and inv_item_sk = i_item_sk
 and d_date_sk=inv_date_sk
 and d_date between cast('1998-07-13' as date) and (cast('1998-07-13' as date) +  interval '60 days')
 and i_manufact_id in (731,990,866,908)
 and inv_quantity_on_hand between 100 and 500
 and cs_item_sk = i_item_sk
 group by i_item_id,i_item_desc,i_current_price
 order by i_item_id
 limit 100;

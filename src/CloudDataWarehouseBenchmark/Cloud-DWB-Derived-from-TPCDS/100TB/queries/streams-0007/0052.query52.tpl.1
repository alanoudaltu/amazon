
select /* query_templates/query52.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0007.i0052.1.0:CF! */  dt.d_year
 	,public.item.i_brand_id brand_id
 	,public.item.i_brand brand
 	,sum(ss_ext_sales_price) ext_price
 from public.date_dim dt
     ,public.store_sales store_sales
     ,public.item
 where dt.d_date_sk = store_sales.ss_sold_date_sk
    and store_sales.ss_item_sk = item.i_item_sk
    and item.i_manager_id = 1
    and dt.d_moy=11
    and dt.d_year=1998
 group by dt.d_year
 	,public.item.i_brand
 	,public.item.i_brand_id
 order by dt.d_year
 	,ext_price desc
 	,brand_id
limit 100 ;

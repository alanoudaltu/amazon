
select /* query_templates/query03.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0001.i0003.1.0:CF! */  dt.d_year 
       ,public.item.i_brand_id brand_id 
       ,public.item.i_brand brand
       ,sum(ss_ext_discount_amt) sum_agg
 from  public.date_dim dt 
      ,public.store_sales store_sales
      ,public.item
 where dt.d_date_sk = store_sales.ss_sold_date_sk
   and store_sales.ss_item_sk = item.i_item_sk
   and item.i_manufact_id = 382
   and dt.d_moy=11
 group by dt.d_year
      ,public.item.i_brand
      ,public.item.i_brand_id
 order by dt.d_year
         ,sum_agg desc
         ,brand_id
 limit 100;

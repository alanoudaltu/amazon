
select /* query_templates/query32.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0005.i0032.1.0:CF! */  sum(cs_ext_discount_amt)  as "excess discount amount" 
from 
   public.catalog_sales 
   ,public.item 
   ,public.date_dim
where
i_manufact_id = 124
and i_item_sk = cs_item_sk 
and d_date between '2000-02-06' and 
        (cast('2000-02-06' as date) + interval '90 days')
and d_date_sk = cs_sold_date_sk 
and cs_ext_discount_amt  
     > ( 
         select 
            1.3 * avg(cs_ext_discount_amt) 
         from 
            public.catalog_sales 
           ,public.date_dim
         where 
              cs_item_sk = i_item_sk 
          and d_date between '2000-02-06' and
                             (cast('2000-02-06' as date) + interval '90 days')
          and d_date_sk = cs_sold_date_sk 
      ) 
limit 100;

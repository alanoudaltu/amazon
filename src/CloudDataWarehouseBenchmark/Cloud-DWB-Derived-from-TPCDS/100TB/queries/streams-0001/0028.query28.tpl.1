
select /* query_templates/query28.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0001.i0028.1.0:CF! */  *
from (select avg(ss_list_price) B1_LP
            ,count(ss_list_price) B1_CNT
            ,count(distinct ss_list_price) B1_CNTD
      from public.store_sales
      where ss_quantity between 0 and 5
        and (ss_list_price between 36 and 36+10 
             or ss_coupon_amt between 4889 and 4889+1000
             or ss_wholesale_cost between 4 and 4+20)) B1,
     (select avg(ss_list_price) B2_LP
            ,count(ss_list_price) B2_CNT
            ,count(distinct ss_list_price) B2_CNTD
      from public.store_sales
      where ss_quantity between 6 and 10
        and (ss_list_price between 32 and 32+10
          or ss_coupon_amt between 5852 and 5852+1000
          or ss_wholesale_cost between 5 and 5+20)) B2,
     (select avg(ss_list_price) B3_LP
            ,count(ss_list_price) B3_CNT
            ,count(distinct ss_list_price) B3_CNTD
      from public.store_sales
      where ss_quantity between 11 and 15
        and (ss_list_price between 20 and 20+10
          or ss_coupon_amt between 5575 and 5575+1000
          or ss_wholesale_cost between 37 and 37+20)) B3,
     (select avg(ss_list_price) B4_LP
            ,count(ss_list_price) B4_CNT
            ,count(distinct ss_list_price) B4_CNTD
      from public.store_sales
      where ss_quantity between 16 and 20
        and (ss_list_price between 177 and 177+10
          or ss_coupon_amt between 9731 and 9731+1000
          or ss_wholesale_cost between 74 and 74+20)) B4,
     (select avg(ss_list_price) B5_LP
            ,count(ss_list_price) B5_CNT
            ,count(distinct ss_list_price) B5_CNTD
      from public.store_sales
      where ss_quantity between 21 and 25
        and (ss_list_price between 135 and 135+10
          or ss_coupon_amt between 10472 and 10472+1000
          or ss_wholesale_cost between 35 and 35+20)) B5,
     (select avg(ss_list_price) B6_LP
            ,count(ss_list_price) B6_CNT
            ,count(distinct ss_list_price) B6_CNTD
      from public.store_sales
      where ss_quantity between 26 and 30
        and (ss_list_price between 150 and 150+10
          or ss_coupon_amt between 10354 and 10354+1000
          or ss_wholesale_cost between 67 and 67+20)) B6
limit 100;

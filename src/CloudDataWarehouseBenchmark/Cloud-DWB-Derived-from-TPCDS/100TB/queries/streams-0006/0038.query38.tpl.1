
select /* query_templates/query38.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0006.i0038.1.0:CF! */  count(*) from (
    select distinct c_last_name, c_first_name, d_date
    from public.store_sales store_sales,public.date_dim, public.customer
          where store_sales.ss_sold_date_sk = date_dim.d_date_sk
      and store_sales.ss_customer_sk = customer.c_customer_sk
      and d_month_seq between 1177 and 1177 + 11
  intersect
    select distinct c_last_name, c_first_name, d_date
    from public.catalog_sales,public.date_dim, public.customer
          where catalog_sales.cs_sold_date_sk = date_dim.d_date_sk
      and catalog_sales.cs_bill_customer_sk = customer.c_customer_sk
      and d_month_seq between 1177 and 1177 + 11
  intersect
    select distinct c_last_name, c_first_name, d_date
    from public.web_sales,public.date_dim, public.customer
          where web_sales.ws_sold_date_sk = date_dim.d_date_sk
      and web_sales.ws_bill_customer_sk = customer.c_customer_sk
      and d_month_seq between 1177 and 1177 + 11
) hot_cust
limit 100;


select /* query_templates/query84.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0008.i0084.1.0:CF! */  c_customer_id as customer_id
       , coalesce(c_last_name,'') || ', ' || coalesce(c_first_name,'') as customername
 from public.customer
     ,public.customer_address
     ,public.customer_demographics
     ,public.household_demographics
     ,public.income_band
     ,public.store_returns
 where ca_city	        =  'Brownsville'
   and c_current_addr_sk = ca_address_sk
   and ib_lower_bound   >=  24398
   and ib_upper_bound   <=  24398 + 50000
   and ib_income_band_sk = hd_income_band_sk
   and cd_demo_sk = c_current_cdemo_sk
   and hd_demo_sk = c_current_hdemo_sk
   and sr_cdemo_sk = cd_demo_sk
 order by c_customer_id
 limit 100;
